#include <bits/stdc++.h>
using namespace std;

string reverseString(string str) {
    reverse(str.begin(), str.end());
    return str;
}

int main() {
    string str = "hello";
    string reversed = reverseString(str);
    cout << "Reversed string: " << reversed << endl; // Output should be "olleh"
    return 0;
}
